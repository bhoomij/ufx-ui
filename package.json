{
  "name": "ufx-ui",
  "version": "0.2.6",
  "description": "A UI toolkit to build trading interface",
  "keywords": [
    "exchange",
    "trading-ui",
    "microfinex-ui",
    "front-end"
  ],
  "author": "Bitfinex",
  "license": "Apache-2.0",
  "repository": "https://github.com/bitfinexcom/ufx-ui",
  "main": "dist/ufx-ui.cjs.js",
  "module": "dist/ufx-ui.esm.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "bundle:sass": "scss-bundle -e src/ufx-ui.scss -o src/ufx-ui.bundle.scss -p ./",
    "build:sass": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ufx-ui.bundle.scss src/ufx-ui.css",
    "prefix:css": "postcss --use autoprefixer --use postcss-discard-comments -o dist/css/ufx-ui.css src/ufx-ui.css",
    "compress:css": "node-sass-chokidar --output-style compressed dist/css/ufx-ui.css dist/css/ufx-ui.min.css",
    "build:css": "run-s bundle:sass build:sass prefix:css compress:css",
    "watch:sass": "npm run build:sass && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ufx-ui.scss src/ufx-ui.css --watch --recursive",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "run-s build build:css",
    "cy:test": "cd ./cypress-tests && npm run cy:run",
    "example:build": "cd ./example && npm run build",
    "verify": "run-s lint prepare example:build build:storybook test:ufx-ui",
    "test:snapshots": "cross-env react-scripts test storyshots.test --watchAll=false",
    "test:snapshots:watch": "cross-env react-scripts test storyshots.test",
    "test:unit": "cross-env react-scripts test --watchAll=false",
    "test:unit:watch": "cross-env react-scripts test",
    "test:unit:ready": "cross-env react-scripts test src/components/ui src/components/format src/components/Book src/components/OrderHistory --watchAll=false",
    "test:unit:ready:watch": "cross-env react-scripts test src/components/ui src/components/format src/components/Book src/components/OrderHistory",
    "test:ufx-ui": "run-s test:snapshots test:unit:ready",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "install:peer": "install-peers -f",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "deploy:storybook": "storybook-to-ghpages --remote=upstream",
    "docz:dev": "docz dev",
    "docz:clean": "rimraf docz-build",
    "docz:build": "npm run build:sass && npm run docz:clean && docz build",
    "docz:serve": "docz build && docz serve"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.10.1",
    "@mdx-js/mdx": "^1.6.16",
    "@rollup/plugin-url": "5.0.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "@storybook/storybook-deployer": "^2.8.7",
    "@svgr/rollup": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.1.1",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-plugin-svgr": "^2.0.2",
    "husky": "^4.3.8",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.1.0",
    "jest-canvas-mock": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-discard-comments": "^4.0.2",
    "prettier": "^2.0.4",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "2.6.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-size-snapshot": "0.11.0",
    "rollup-plugin-strip": "1.2.2",
    "rollup-plugin-terser": "5.3.0",
    "rollup-plugin-visualizer": "4.0.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@popperjs/core": "^2.7.0",
    "axios": "^0.21.1",
    "base-64": "^0.1.0",
    "bignumber.js": "^9.0.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "focus-trap-react": "^7.0.1",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "i18next": "^19.5.1",
    "jest-enzyme": "^7.1.2",
    "lodash": "^4.17.15",
    "markdown-to-jsx": "^7.1.2",
    "memoize-one": "^5.1.1",
    "node-sass-chokidar": "^1.5.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "rc-slider": "^9.6.4",
    "react-beforeunload": "^2.2.4",
    "react-datepicker": "^3.3.0",
    "react-grid-layout": "^1.1.1",
    "react-i18next": "^11.7.0",
    "react-measure": "^2.3.0",
    "react-number-format": "^4.4.1",
    "react-popper-tooltip": "^2.11.1",
    "react-router-dom": "^5.2.0",
    "react-throttle-render": "^2.0.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-action-types": "^2.0.0",
    "redux-batched-subscribe": "^0.1.6",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "scss-bundle": "^3.1.2",
    "yup": "^0.29.1"
  },
  "browserslist": [
    "last 4 versions"
  ]
}
